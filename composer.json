{
	"name": "marcinlawnik/DziennikLoginWebNew",
	"description": "DziennikLogin web interface in laravel",
	"license": "unlicense",
	"require": {
		"laravel/framework": "4.1.*",
        "twitter/bootstrap": "*",
        "codesleeve/asset-pipeline": "dev-master",
        "way/generators": "2.*",
        "jyggen/curl": "v3.0.1",
        "yangqi/htmldom": "dev-master"
    },
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
            "app/jobs"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
